/* Content */

.container {
    width:  100%;
    max-width:  1000px !important;
    margin:  0px auto;
}

    .secondary .container > .padding {
        padding-top:  50px;
        padding-bottom:  50px;
        clear: both;
    }
    
        .secondary .section.text.nopadding .container > .padding {
            padding-bottom: 0px;
        }
    
    .container.section.default {
        margin-bottom:  50px;
    }

.marquee {
    padding:  0px 0px;
    background-color:  #25b7ea;
    color: #fff;
    font-family: Aleo, Times New Roman, serif;
    font-size: 12pt;
    line-height: 16pt;
    position: relative;
}

    .secondary .marquee h1 {
        color: #fff;
        padding-bottom: 0px;
    }

	.marquee .copy-slider {
		position: relative;
		left: -200%;
		 transition: left left .5s linear .5s;
        -webkit-transition: left .5s linear .5s;
        -moz-transition: left .5s linear .5s;
	}
		
		.marquee .copy-slider.loaded {
			left: 0%;
		}
		
    .marquee .image {
        position: absolute;
        width:  62%;
        right:  -100%;
        top:  0px;
        bottom:  0px;
        background-size: cover;
        -ms-behavior: url(/backgroundsize.min.htc);
        background-position:  center center;
        z-index: 10;
        transition: right .5s linear .5s;
        -webkit-transition: right .5s linear .5s;
        -moz-transition: right .5s linear .5s;
    }
    
    	.marquee .image.loaded {
    		right: 0%;
    	}
    	
    
        .marquee .image .title {
            display: none;
            font-family: Source Sans, Arial, Verdana, sans-serif;
            font-size:  28pt;
            line-height: 26pt;
            padding:  45px 10px;
        }
    
        .marquee .image .border {
            width:  330px;
            position: absolute;
            left:  0px;
            top:  0px;
            bottom: 0px;
            background-image:  url(images/marquee-border.png);
            background-position: top right;
        }
        
        @media all and (max-width: 481px) {
            .marquee .image {
                position: relative;
                width:  100%;
            }
            .marquee .image .border {
                display: none;
            }
            .marquee .image .title {
                display: block;
                text-shadow: 2px 2px 2px #000;
                font-weight: bold;
                width: 95%;
            }
            
            
            /* Remove space for pages without image in header */
            div.image:not([style]) + div.container div {
				display:none;
			}
			
        }
        
        @media all and (min-width: 481px) and (max-width: 768px) {
            .marquee .image {
                width:  50%;
            }
            .marquee .grid_5 {
                width:  60%;
            }
            
            .marquee .copy p:last-of-type {
            	width: 95%;
            }
        }
        
        @media all and (min-width: 769px) and (max-width: 900px) {
            .marquee .image {
                width:  55%;
            }
            .marquee .grid_5 {
                width:  50%;
            }
        }

    .marquee .copy{
        padding:  50px 0px;
        position: relative;
        z-index: 1000;
    }

        .marquee .copy h1 {
            font-family: Source Sans, Arial, Verdana, sans-serif;
            font-size:  28pt;
            line-height: 28pt;
            /*padding-bottom: 20px;*/
            color: #fff;
        }
        
        @media all and (max-width: 481px) {
            .marquee .copy h1 {
                display: none;
            }
        }
        
        .marquee .copy .content {
            margin-top: 20px;
            font-weight: bold;
        }
        
