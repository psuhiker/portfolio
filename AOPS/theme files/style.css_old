/* Buttons */

a.button {
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    font-family: Source Sans, Arial, Verdana, sans-serif;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
}

    a.button:hover {
        transition: all 0.5s;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
    }

a.button.small {
    padding:  5px 20px;
    font-size: 12pt;
}

a.button.medium {
    padding:  8px 25px;
    font-size: 12pt;
    display: inline-block;
}

a.button.large {
    padding:  10px 30px;
    font-size: 14pt;
}

a.button.blue {
    background-color: #25b7ea;
    color: #fff;
    text-decoration: none;
    opacity: 1;
}

    a.button.blue:hover {
        opacity: 0.65;
    }
    
        a.button.blue.medium {
            border:  1px solid #25b7ea;
        }
    
            a.button.blue.medium:hover {
                border:  1px solid #25b7ea;
                background-color: #fff;
                color: #25b7ea;
            }
            
    		@media (max-width: 480px) {
    			a.button.blue.medium {
    				margin-bottom: 25px;
    			}
    		}
    a.button.blue.large {
        display: inline-block;
        vertical-align: middle;
        -webkit-transform: translateZ(0);
        transform: translateZ(0);
        box-shadow: 0 0 1px rgba(0, 0, 0, 0);
        -webkit-backface-visibility: hidden;
        backface-visibility: hidden;
        -moz-osx-font-smoothing: grayscale;
        -webkit-transition-duration: 0.3s;
        transition-duration: 0.3s;
        -webkit-transition-property: transform;
        transition-property: transform;
        transition: all 0.5s;
        -webkit-transition: all 0.5s;
        -moz-transition: all 0.5s;
    }
    
        a.button.blue.large:hover, a.button.blue.large:active {
            -webkit-transform: scale(1.1);
            transform: scale(1.1);
            opacity: 1;
            transition: all 0.5s;
            -webkit-transition: all 0.5s;
            -moz-transition: all 0.5s;
        }
    
a.button.red {
    background-color: #eb1c4d;
    color: #fff;
    text-decoration: none;
    opacity: 1;
